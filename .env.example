# Example environment variables for LifeQuest backend
# Copy to .env and edit values as needed

# MongoDB connection string (Atlas or local)
# Example Atlas URI:
# MONGODB_URI=mongodb+srv://<user>:<password>@cluster0.example.mongodb.net/lifequest?retryWrites=true&w=majority

# Example local Docker URI:
MONGODB_URI=mongodb://localhost:27017/lifequest

# Server port (optional)
PORT=3001
